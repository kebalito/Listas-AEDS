01.
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int i;
    int num[21];
    int vet;
    srand(time(NULL));
    for(i=0; i<20; i++)
    {
    num[i]= 11+rand()%88;
    printf(" %d\n",num[i]);
    }
    return 0;
}

02.
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int i;
    int num[21];
    int vet;
    srand(time(NULL));
    for(i=0; i<20; i++)
    {
    num[i]= 11+rand()%88;
    printf("Vetor [%d] = %d\n",i,num[i]);
    }
    return 0;
}

